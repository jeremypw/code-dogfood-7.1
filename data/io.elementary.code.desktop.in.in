[Desktop Entry]
Type=Application
Name=@NAME@
Comment=Dogfood Code editor on eOS7
GenericName=Experimental Code Editor
Exec=@EXEC_NAME@ %U
Icon=missing-image
Terminal=false
Categories=Development;GTK;IDE;WebDevelopment;
Keywords=text;IDE;scratch;code;
MimeType=text/plain;inode/directory;
StartupNotify=true
Actions=NewWindow;NewFile;

[Desktop Action NewFile]
Name=New File
Exec=@EXEC_NAME@ --new-tab

[Desktop Action NewWindow]
Name=New Window
Exec=@EXEC_NAME@ --new-window
